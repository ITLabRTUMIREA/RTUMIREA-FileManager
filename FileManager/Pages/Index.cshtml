@page
@model IndexModel
@using FileManager.Models.Database.YearDocumentTitles;
@{
    ViewData["Title"] = "Home page";
    Layout = "Shared/_MainApplicationLayout";
}
<div class="row">
    @foreach (Year year in Model.Years)
    {
        <div class="col-sm-1 mx-3">
            <a asp-page="/Directory/Year" asp-route-yearId="@year.Id">
                @year.Number
            </a>
        </div>
    }
</div>
